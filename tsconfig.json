{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [
      "es5",
      "es2015.iterable",
      "es2015.collection",
      "es2015.promise"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "module": "es6",
    "target": "es6",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": false,
    "baseUrl": ".", // This must be specified if "paths" is.
    "typeRoots" : ["typings"]
  }
}
